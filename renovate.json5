    {
      groupName: "remark docs linting",
      updateTypes: ["major", "minor", "patch", "pin"],
      paths: ["package.json"],
      packageNames: ["remark", "retext"],
      packagePatterns: ["^remark-", "^retext-"],
      dependencyDashboardApproval: false,
